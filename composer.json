{
    "name": "xcalder/laravel-scout-xunsearch",
    "description": "Xunsearch Driver for Laravel Scout.",
    "keywords": ["Lumen", "scout", "xunsearch"],
    "authors": [
        {
            "name": "xcalder",
            "email": "xcalder@foxmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/scout": "^7|^8"
    },
    "autoload": {
        "psr-4": {
            "Scout\\Xunsearch\\": "src/"
        },
        "classmap": ["sdk"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Scout\\Xunsearch\\XunsearchServiceProvider"
            ],
            "aliases" : {
				"Xunsearch" : "Scout\\Xunsearch\\Facades\\Xunsearch"
			}
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true
}
